      "typeorm-seeding": "1.0.0-beta.6",
      "typeorm-typedi-extensions": "^0.2.3",
      "typescript": "^3.6.3",
      "winston": "^3.2.1"
   },
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/dotenv": "^6.1.1",
      "@types/figlet": "^1.2.0",
      "@types/helmet": "^0.0.44",
      "@types/jest": "^24.0.18",
      "@types/serve-favicon": "^2.2.31",
      "@types/uuid": "^3.4.5",
      "cross-env": "^5.2.1",
      "jest": "^24.9.0",
      "mock-express-request": "^0.2.0",
      "mock-express-response": "^0.2.1",
      "ncp": "^2.0.0",
      "nock": "10.0.0",
      "supertest": "^4.0.2",
      "ts-jest": "^24.1.0"
   },
   "resolutions": {
      "**/event-stream": "^4.0.1"
   },
   "jest": {
      "transform": {
         ".(ts|tsx)": "<rootDir>/test/preprocessor.js"
      },
      "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "json"
      ],
      "testEnvironment": "node",
      "setupFilesAfterEnv": [
         "./test/unit/lib/setup.ts"
      ]
   },
   "license": "MIT"
}
