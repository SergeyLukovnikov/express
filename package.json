      "copyfiles": "^2.1.1",
      "cors": "^2.8.5",
      "cron": "^1.7.2",
      "dataloader": "^1.4.0",
      "dotenv": "^8.1.0",
      "event-dispatch": "^0.4.1",
      "express": "^4.17.1",
      "express-basic-auth": "^1.2.0",
      "express-status-monitor": "^1.2.6",
      "faker": "^4.1.0",
      "figlet": "^1.2.4",
      "glob": "^7.1.4",
      "helmet": "^3.21.0",
      "jsonfile": "^5.0.0",
      "microframework-w3tec": "^0.6.3",
